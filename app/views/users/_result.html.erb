<% if @stock %>
  <div class="well results-block">
    <strong>Name: </strong><%= @stock.name %><br>
    <strong>Latest Price: </strong><%= @stock.last_price %>
    <br><br>
    <%= area_chart @filtered_data, prefix: "$", discrete: true, xtitle: "Eastern Standard Time",
                    points: false, min: @stock.low.to_i-1, max: @stock.high.to_i+1 %>
    <%# TODO extend x axis to end of day %>
  </div>
<% end %>
